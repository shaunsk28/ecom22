   <script>
        // Get all the menu items
        const menuItems = document.querySelectorAll('.menu-aside li');

        // Add event listeners to each menu item
        menuItems.forEach(menuItem => {
            menuItem.addEventListener('click', function(event) {
                // Remove the 'active' class from all menu items
                menuItems.forEach(item => item.classList.remove('active'));

                // Add the 'active' class to the clicked menu item
                this.classList.add('active');

                // Store the active tab in localStorage
                localStorage.setItem('activeTab', this.getAttribute('data-tab'));
            });
        });

        // Check if there is a stored active tab and add the 'active' class to it
        const storedActiveTab = localStorage.getItem('activeTab');
        if (storedActiveTab) {
            const activeTabElement = document.querySelector(`.menu-aside li[data-tab="${storedActiveTab}"]`);
            if (activeTabElement) {
                activeTabElement.classList.add('active');
            }
        }
    </script>